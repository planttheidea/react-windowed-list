{
  "author": "tony.quetano@planttheidea.com",
  "ava": {
    "babel": "inherit",
    "failFast": true,
    "files": ["test/*.js", "!test/helpers/setup-browser-env.js"],
    "require": ["babel-register", "babel-polyfill", "test/helpers/setup-browser-env.js"],
    "verbose": true
  },
  "bugs": {
    "url": "https://github.com/planttheidea/react-windowed-list/issues"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "moize": "^3.3.0 || ^4.0.0",
    "prop-types": "^15.6.0",
    "raf": "^3.3.2",
    "react": "^15.3.0 || ^16.0.0",
    "react-dom": "^15.3.0 || ^16.0.0",
    "remeasure": "^2.4.1"
  },
  "description": "A fast, versatile virtual-render list component for React",
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "browser-env": "^3.2.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "enzyme-to-json": "^3.0.1",
    "eslint": "^4.6.0",
    "eslint-config-rapid7": "^2.11.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "html-webpack-plugin": "^2.30.1",
    "in-publish": "^2.0.0",
    "lodash-webpack-plugin": "^0.11.4",
    "nyc": "^11.1.0",
    "optimize-js-plugin": "^0.0.4",
    "react-test-renderer": "^16.0.0",
    "sinon": "^4.0.0",
    "uuid": "^3.1.0",
    "webpack": "^3.5.5",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.7.1"
  },
  "homepage": "https://github.com/planttheidea/react-windowed-list#readme",
  "keywords": ["react", "reactjs", "virtual", "list", "scrolling", "infinite", "virtualized", "table", "grid"],
  "license": "MIT",
  "main": "lib/index.js",
  "name": "react-windowed-list",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/react-windowed-list.git"
  },
  "scripts": {
    "build": "NODE_ENV=development webpack --progress --colors",
    "build:minified": "NODE_ENV=production webpack --progress --colors --config=webpack.config.minified.js",
    "dev": "NODE_ENV=development webpack-dev-server --colors --config=webpack.config.dev.js",
    "lint": "NODE_ENV=test eslint src",
    "lint:fix": "NODE_ENV=test eslint src --fix",
    "prepublish": "if in-publish; then npm run prepublish:compile; fi",
    "prepublish:compile":
      "npm run lint && npm run test:coverage && npm run transpile && npm run build && npm run build:minified",
    "start": "npm run dev",
    "test": "NODE_PATH=. NODE_ENV=test ava --no-cache",
    "test:coverage": "nyc npm test",
    "test:update": "NODE_PATH=. NODE_ENV=test ava --update-snapshots",
    "test:watch": "NODE_PATH=. NODE_ENV=test ava --watch",
    "transpile": "NODE_ENV=production babel src --out-dir lib"
  },
  "version": "1.5.5"
}
